{
  "name": "text-input-validator",
  "version": "0.1.1",
  "description": "Progressive text input control validator",
  "main": "TextInputValidator.js",
  "scripts": {
    "build": "rollup -c rollup.config.TextInputValidator.js && rollup -c rollup.config.Core.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/text-input-validator.git"
  },
  "keywords": [
    "text",
    "form",
    "validator"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/text-input-validator/issues"
  },
  "homepage": "https://github.com/fenivana/text-input-validator#readme",
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-enough": "0.2.6",
    "rollup": "0.42.0",
    "rollup-plugin-babel": "2.7.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
